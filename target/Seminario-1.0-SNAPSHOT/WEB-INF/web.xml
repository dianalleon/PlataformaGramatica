        <servlet-name>Cancelar</servlet-name>
        <url-pattern>/Cancelar.do</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CancelarOperation</servlet-name>
        <url-pattern>/CancelarOperation.do</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
